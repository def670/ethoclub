{{#if label}}
  {{#paper-tab/label parentComponent=this}}{{label}}{{/paper-tab/label}}
  {{#paper-tab/body parentComponent=this}}{{yield}}{{/paper-tab/body}}
{{else}}
  {{yield (hash
    label=(component "paper-tab/label" parentComponent=this)
    body=(component "paper-tab/body" parentComponent=this)
  )}}
{{/if}}
